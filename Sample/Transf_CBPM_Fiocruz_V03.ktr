String scientificnameField;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	// First, get a row from the default input hop
    Object[] r = getRow();

    // If the row object is null, we are done processing.
    if (r == null) {

      setOutputDone();

      return false;

    }
	// Let's look up parameters only once for performance reason.
    if (first) {

      scientificnameField = getParameter("SCIENTIFICNAME_TAG");

      first=false;

    }

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.

    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

    String scientificname = get(Fields.In, scientificnameField).getString(r);

    String introductionDerived= "''''''"+scientificname+"''''''"+" "
			+"é uma [[espécie]] de planta que possui registro de ocorrência no [[Brasil]], "
			+" sendo de uso medicinal. Esta [[espécie]] faz parte da Coleção Botânica de Plantas da Fundação Oswaldo Cruz (Fiocruz). ";
    get(Fields.Out, "introduction").setValue(outputRow, introductionDerived);
    putRow(data.outputRowMeta, outputRow);


    return true;
}
